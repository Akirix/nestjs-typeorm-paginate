    "@types/jest": "^24.0.11",
    "coveralls": "^3.0.3",
    "jest": "^24.7.1",
    "prettier": "^1.16.2",
    "ts-jest": "^24.0.2",
    "typeorm": "^0.2.12",
    "typescript": "^3.2.4"
  },
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "coveralls": "yarn run test:cov --coverageReporters=text-lcov | coveralls",
    "test:cov": "jest --coverage",
    "build": "tsc -p ."
